<% content_for :page_actions do %>
  <% if @return_to_product.nil?%>
    <% if can?(:create, Spree::SalesSchedule) %>
      <%= button_link_to Spree.t(:new_sales_schedule), new_object_url, { class: "btn-success", icon: 'add', id: 'admin_new_sales_schedule' } %>
    <% end %>
  <% end %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @sales_schedule } %>

<%= form_for [:admin, @sales_schedule], method: :put, html: { multipart: true } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>